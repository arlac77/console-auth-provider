{
  "name": "console-auth-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/auth-provider.mjs",
  "exports": {
    ".": "./src/auth-provider.mjs"
  },
  "description": " Provide credentials from console user input",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/auth-provider.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/auth-provider.mjs"
  },
  "dependencies": {
    "enquirer": "^2.3.5"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.2.3"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/console-auth-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/console-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/console-auth-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
